<?xml version="1.0"?>
<Clipboard>
  <clipboard last="true" >Put_Line(Float&apos;Image(error));</clipboard>
  <clipboard>:= Mars_Lander.Lander.getPosition.y;</clipboard>
  <clipboard>error := Mars_Lander.Lander.getPosition.y;</clipboard>
  <clipboard>error :Float := Mars_Lander.Lander.getPosition.y;</clipboard>
  <clipboard>error</clipboard>
  <clipboard>Mars_Lander.Lander.getPosition.x</clipboard>
  <clipboard>elsif Mars_Lander.Lander.getPosition.x &lt; 20.0 then
            Mars_Lander.inputFlags.setUp(true);</clipboard>
  <clipboard>Mars_Lander.inputFlags.setUp(false);</clipboard>
  <clipboard>Mars_Lander.Lander.getPosition.x &gt; 20.0</clipboard>
  <clipboard>Mars_Lander.inputFlags.setUp(Get_Key_Status(SDLK_UP));</clipboard>
</Clipboard>
